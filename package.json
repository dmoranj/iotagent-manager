{
  "name": "iotagent-manager",
  "description": "IoT Agent Manager proxy",
  "version": "1.7.0-next",
  "homepage": "https://github.com/dmj/iotagent-manager",
  "author": {
    "name": "Daniel Moran",
    "email": "dmj@tid.es"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/dmj/iotagent-manager.git"
  },
  "bugs": {
    "url": "https://github.com/dmj/iotagent-manager/issues"
  },
  "main": "lib/iotagent-manager",
  "engines": {
    "node": ">=8.12.0"
  },
  "scripts": {
    "clean": "rm -rf package-lock.json && rm -rf node_modules && rm -rf coverage",
    "test": "mocha --recursive 'test/**/*.js' --reporter spec --timeout 3000 --ui bdd --exit",
    "test:watch": "npm run test -- -w ./lib",
    "lint": "jshint lib/ --config .jshintrc && jshint test/ --config test/.jshintrc",
    "test:coverage": "istanbul cover _mocha -- --recursive 'test/**/*.js' --reporter spec --exit",
    "watch": "watch 'npm test && npm run lint' ./lib ./test"
  },
  "dependencies": {
    "request": "2.88.0",
    "body-parser": "~1.11.0",
    "express": "4.16.4",
    "logops": "2.1.0",
    "underscore": "~1.7.0",
    "revalidator": "~0.3.1",
    "mongoose": "4.13.14",
    "async": "2.0.1",
    "iotagent-node-lib": "git://github.com/telefonicaid/iotagent-node-lib.git#task/lazyAttsNgsiv2-pr762"
  },
  "devDependencies": {
    "jshint": "~2.9.6",
    "mocha": "5.2.0",
    "mongodb": "2.2.35",
    "should": "8.2.2",
    "istanbul": "0.4.5",
    "timekeeper": "0.0.5",
    "nock": "9.0.14",
    "watch": "~1.0.2"
  },
  "keywords": []
}
